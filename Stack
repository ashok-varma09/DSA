#include <iostream>
using namespace std;

char stack[9] = {'{','5','+','(','2','+','3',')','}'};
int stackIndex=0;

int Pop()
{
	stackIndex--;
	return stack[stackIndex];
}

void Push(int value)
{
	stack[stackIndex] = value;
	stackIndex++;
}

int Top()
{
	return stack[stackIndex-1];
}

int main()
{
	stackIndex;
	int length,j=0;
	int operands1,operands2;

	char op;
	length = sizeof(stack)/sizeof(stack[2]);



	for (int i=0; i<length; i++)
	{
		Push(stack[i]);

		if(stack[i]=='(')// {5+(3+1)}
		{
            Pop();
			j=i;
        
			while(stack[i]!=')')
			{

				operands1=Pop()-'0';

				op =Pop();

				operands2 = Pop()-'0';


				Push(stack[i]);
				Pop();
				i++;
			}


		}

	}

    
    int ans;
	if(op=='+')
	{
		 ans = operands1+operands2;
	}


	int k;
	
	
	
    int op1,operetor;
	
	for(int i=0; i<stackIndex; i++)
	{

		if(stack[i]=='{')
		{
		   
			k=i;
			while(stack[i]!='}')
			{
			   
			   op1=Pop() - '0';
			   
			   operetor = Pop();
			   
			  
			   
			   
			   
			   Pop();
                
                i++;

			}   
		}
	}



	if(operetor=='+')
	{
	   int answer = op1 + ans;
	   cout<<answer;
	    
	}



    cout<<"Top : "<<Top()<<endl;


	return 0;
}
